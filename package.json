{
  "name": "lorem-alan",
  "displayName": "Lorem Alan",
  "description": "Generate Alan Partridgesque Lorem Ipsum text",
  "version": "0.0.2",
  "publisher": "JohnnyHall",
  "engines": {
    "vscode": "^1.92.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:lorem-alan.generate"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "lorem-alan.generate",
        "title": "Generate Lorem Alan Text"
      }
    ],
    "keybindings": [
      {
        "command": "lorem-alan.generate",
        "key": "ctrl+shift+alt+a",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.92.0",
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0"
  },
  "icon": "images/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/jbhall4291/lorem-alan-vscode-extension"
  }
}